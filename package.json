{
  "name": "connect",
  "version": "0.8.0",
  "scripts": {
    "start": "craco start",
    "build:development": "env-cmd .env.development craco build",
    "build:production": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` env-cmd .env.production craco build",
    "test": "craco test --env=jsdom",
    "test-ci": "CI=true craco test --env=jsdom",
    "test-coverage": "CI=true craco test --env=jsdom --coverage",
    "test-puppeteer": "jest -c src/__puppeteer__/jest.config.js",
    "test-puppeteer-build": "JEST_PUPPETEER_CONFIG=jest-puppeteer.build.config.js yarn run test-puppeteer",
    "deploy": "npm run test-ci && npm run build:production && npm run test-puppeteer-build && gh-pages -d build"
  },
  "repository": "git@github.com:commaai/connect.git",
  "author": "Chris Vickery <chrisinajar@gmail.com>",
  "private": true,
  "dependencies": {
    "@commaai/comma-api": "^2.6.0",
    "@commaai/hls.js": "^0.12.7",
    "@commaai/my-comma-auth": "^1.4.1",
    "@mapbox/mapbox-sdk": "^0.13.5",
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "@sentry/react": "^7.14.0",
    "@sentry/webpack-plugin": "^1.19.0",
    "connected-react-router": "^4.3.0",
    "debounce": "^1.2.1",
    "fecha": "^3.0.3",
    "gh-pages": "^4.0.0",
    "global": "^4.4.0",
    "history": "^4.10.1",
    "jsonwebtoken": "^8.5.1",
    "localforage": "^1.10.0",
    "obstruction": "^2.1.0",
    "photo-sphere-viewer": "^4.7.3",
    "prop-types": "^15.8.1",
    "qr-scanner": "^1.4.1",
    "query-string": "^6.14.1",
    "raf": "^3.4.1",
    "react": "~16.8",
    "react-dom": "~16.8",
    "react-map-gl": "^5.3.15",
    "react-measure": "^2.5.2",
    "react-player": "^2.11.0",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.2.23",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.4.4",
    "react-virtualized": "^9.22.3",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@craco/craco": "^5.5.0",
    "babel-eslint": "^10.0.3",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "expect-puppeteer": "^5.0.4",
    "jest-puppeteer": "^5.0.4",
    "puppeteer": "^9.1.1",
    "serve": "^14.0.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^@commaai/pandajs$": "<rootDir>/node_modules/@commaai/pandajs/lib/index.js",
      "^@commaai/hls.js$": "<rootDir>/node_modules/@commaai/hls.js/dist/hls.js",
      "^@commaai/(.*comma.*)$": "<rootDir>/node_modules/@commaai/$1/dist/index.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
