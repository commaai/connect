{
  "name": "connect",
  "version": "0.8.0",
  "scripts": {
    "start": "craco start",
    "build:development": "env-cmd .env.development craco build",
    "build:production": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` env-cmd .env.production craco build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "test": "craco test --env=jest-environment-jsdom-sixteen",
    "test-ci": "CI=true craco test --env=jest-environment-jsdom-sixteen",
    "test-coverage": "CI=true craco test --env=jest-environment-jsdom-sixteen --coverage",
    "test-puppeteer": "jest -c src/__puppeteer__/jest.config.js",
    "test-puppeteer-build": "JEST_PUPPETEER_CONFIG=jest-puppeteer.build.config.js yarn run test-puppeteer"
  },
  "repository": "git@github.com:commaai/connect.git",
  "author": "Chris Vickery <chrisinajar@gmail.com>",
  "private": true,
  "dependencies": {
    "@commaai/comma-api": "^2.6.0",
    "@commaai/my-comma-auth": "^1.4.1",
    "@craco/craco": "^6.4.3",
    "@mapbox/mapbox-sdk": "^0.13.5",
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "@sentry/react": "^7.14.0",
    "@sentry/webpack-plugin": "^1.19.0",
    "connected-react-router": "^4.5.0",
    "debounce": "^1.2.1",
    "fecha": "^3.0.3",
    "global": "^4.4.0",
    "history": "^4.10.1",
    "jwt-decode": "^3.1.2",
    "localforage": "^1.10.0",
    "obstruction": "^2.1.0",
    "photo-sphere-viewer": "^4.7.3",
    "prop-types": "^15.8.1",
    "qr-scanner": "^1.4.1",
    "query-string": "^6.14.1",
    "raf": "^3.4.1",
    "react": "~17.0",
    "react-dom": "~17.0",
    "react-map-gl": "^5.3.15",
    "react-measure": "^2.5.2",
    "react-player": "^2.11.0",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.2.23",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-scripts": "4.0.3",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "compression-webpack-plugin": "^6.1.1",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.11.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expect-puppeteer": "^5.0.4",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-puppeteer": "^5.0.4",
    "puppeteer": "^9.1.1",
    "serve": "^14.0.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "import/no-named-as-default": 0,
      "react/destructuring-assignment": "warn",
      "react/jsx-curly-spacing": 0,
      "react/jsx-filename-extension": 0,
      "react/forbid-prop-types": 0,
      "react/prop-types": 0,
      "react/require-default-props": 0,
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "object-curly-newline": ["error", { "consistent": true }]
    },
    "env": {
      "browser": true
    },
    "globals": {
      "gtag": "readonly"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "^@commaai/pandajs$": "<rootDir>/node_modules/@commaai/pandajs/lib/index.js",
      "^@commaai/(.*comma.*)$": "<rootDir>/node_modules/@commaai/$1/dist/index.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
